DEVICE:51
;MODULE:Data
;MODULE_TYPE:0
;<h1/>Coating RB
;動作No.確認
;Operation No. check
LD CR2002                ; 常時ON
PMOV D100 K0 Y205 K0 K6  ; RB　指示動作No.  RB IN 5  Program No.1
MOV K0 D110              ; RB　受付動作No.
PMOV X109 K0 D110 K0 K6  ; RB OUT 9  動作No.1  RB　受付動作No.
LD CR2002                ; 常時ON
LDA E55                  ; Type No.
CON
MPS
AND M1603                ; B面加工
ADD K32
MPP
STA D102                 ; 1側塗布指示Prog.No.
CON
ADD K16
CON
STA D104                 ; 2側塗布指示Prog.No.
;<h1/>Palette ID
LD CR2002                ; 常時ON
MPS
PMOV X11A K0 D90 K0 K4   ; ﾊﾟﾚｯﾄID 1  Palette ID
MPP
LD= D90 E55              ; Palette ID  Type No.
AND<> E55 K15            ; Type No.
LD= E55 K14              ; Type No.
AND= D90 K2              ; Palette ID
ORL
ANL
OUT M300                 ; ﾊﾟﾚｯﾄID OK
;<h1/>Current Time data
LD CR2002                ; 常時ON
SEC CM700 D400           ; ｶﾚﾝﾀﾞﾀｲﾏ（年）  現在時刻secﾃﾞｰﾀ
;<h1/>Daily check Flg check
LD>= D110 K1             ; RB　受付動作No.
AND<= D110 K14           ; RB　受付動作No.
LD>= D110 K17            ; RB　受付動作No.
AND<= D110 K30           ; RB　受付動作No.
ORL
LD>= D110 K33            ; RB　受付動作No.
AND<= D110 K46           ; RB　受付動作No.
ORL
LD>= D110 K49            ; RB　受付動作No.
AND<= D110 K63           ; RB　受付動作No.
ORL
AND X103                 ; RB OUT 3  BUSY
OUT @M0                  ; RB 吐出有動作完了
LDB Y20E                 ; RB IN 14 吐出無(ﾄﾚｰｽﾓｰﾄﾞ)
LDB M25                  ; 吐出無し
AND Y20F                 ; RB IN 15 手動:吐出ﾊﾞﾙﾌﾞON
OR @M0                   ; RB 吐出有動作完了
ANL
OUT @M1                  ; 吐出動作有り
LD CR2002                ; 常時ON
CAL* E1010 K60 D402      ; 塗布幅点検時間(min)  日常点検記憶RESET間隔(sec)
CAL* E1012 K10 D404      ; ﾉｽﾞﾙ浸漬待機時間(sec)  ﾉｽﾞﾙ浸漬待機時間(0.0s)
MPS
LDP M40                  ; Dispence Daily check OK
ORB M40                  ; Dispence Daily check OK
ANL
CAL+.D D400 D402 D410    ; 現在時刻secﾃﾞｰﾀ  日常点検記憶RESET間隔(sec)  日常点検記憶RESET予定
MRD
LD @M1                   ; 吐出動作有り
LD= D110 K15             ; RB　受付動作No.
OR= D110 K0              ; RB　受付動作No.
ANB X103                 ; RB OUT 3  BUSY
ORL
ANL
CAL+.D D400 E1012 D412   ; 現在時刻secﾃﾞｰﾀ  ﾉｽﾞﾙ浸漬待機時間(sec)  浸漬予定時間
MRD
CAL-.D D412 K10 D414     ; 浸漬予定時間  浸漬実行警告時間
MPP
RSEC D412 D420           ; 浸漬予定時間
END
ENDH
