DEVICE:128
;MODULE:減圧制御
;MODULE_TYPE:0
;
LD M60                   ; 減圧制御要求
OUT M200                 ; 減圧制御ｽﾀｰﾄ
MC
LD M200                  ; 減圧制御ｽﾀｰﾄ
ANB Y150A                ; 大気開放弁 開
OR M204                  ; 減圧中
ANB M205                 ; 設定圧到達(減圧制御)
ANB M206                 ; 常圧制御要求(減圧中)
ANB M201                 ; 減圧保持開始
OUT M204                 ; 減圧中
LD M204                  ; 減圧中
AND>=.L D0 D25000        ; 運用品種設定1  移動平均真空圧値(kPa)
OR M205                  ; 設定圧到達(減圧制御)
ANB M201                 ; 減圧保持開始
OUT M205                 ; 設定圧到達(減圧制御)
LD M205                  ; 設定圧到達(減圧制御)
OR M201                  ; 減圧保持開始
ANB M206                 ; 常圧制御要求(減圧中)
OUT M201                 ; 減圧保持開始
TMR @0 D2000             ; 保持時間  到達圧保持時間*10
LD M201                  ; 減圧保持開始
CAL+.L D0 D502 D2006     ; 運用品種設定1  減圧調圧設定値  再減圧値(設定値+1)
MPS
LD<=.L D2006 D25000      ; 再減圧値(設定値+1)  移動平均真空圧値(kPa)
OR M207                  ; 再減圧開始
ANB M206                 ; 常圧制御要求(減圧中)
ANB M208                 ; 再減圧到達
ANL
OUT M207                 ; 再減圧開始
MPP
AND>=.L D0 D25000        ; 運用品種設定1  移動平均真空圧値(kPa)
OUT M208                 ; 再減圧到達
LD @T0                   ; 保持時間
OR M206                  ; 常圧制御要求(減圧中)
OUT M206                 ; 常圧制御要求(減圧中)
LD M206                  ; 常圧制御要求(減圧中)
AND M310                 ; 常圧制御完了
OUT M210                 ; 減圧制御完了
;
MCR
END
ENDH
