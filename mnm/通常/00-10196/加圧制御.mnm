DEVICE:128
;MODULE:加圧制御
;MODULE_TYPE:0
;
LD M62                   ; 加圧制御要求
OUT M250                 ; 加圧制御ｽﾀｰﾄ
MC
LD M250                  ; 加圧制御ｽﾀｰﾄ
ANB Y150A                ; 大気開放弁 開
OR M254                  ; 加圧中
ANB M255                 ; 設定圧到達(加圧制御)
ANB M256                 ; 常圧制御要求(加圧中)
ANB M251                 ; 加圧保持開始
OUT M254                 ; 加圧中
LD M254                  ; 加圧中
AND<=.L D0 D25000        ; 運用品種設定1  移動平均真空圧値(kPa)
OR M255                  ; 設定圧到達(加圧制御)
ANB M251                 ; 加圧保持開始
OUT M255                 ; 設定圧到達(加圧制御)
LD M255                  ; 設定圧到達(加圧制御)
OR M251                  ; 加圧保持開始
ANB M256                 ; 常圧制御要求(加圧中)
OUT M251                 ; 加圧保持開始
TMR @0 D2000             ; 保持時間  到達圧保持時間*10
LD M251                  ; 加圧保持開始
CAL-.L D0 D500 D2004     ; 運用品種設定1  加圧調圧設定値  再加圧値(設定値-1)
MPS
LD>=.L D2004 D25000      ; 再加圧値(設定値-1)  移動平均真空圧値(kPa)
OR M257                  ; 再加圧開始
ANB M256                 ; 常圧制御要求(加圧中)
ANB M258                 ; 再加圧到達
ANL
OUT M257                 ; 再加圧開始
MPP
AND<=.L D0 D25000        ; 運用品種設定1  移動平均真空圧値(kPa)
OUT M258                 ; 再加圧到達
LD @T0                   ; 保持時間
OR M256                  ; 常圧制御要求(加圧中)
OUT M256                 ; 常圧制御要求(加圧中)
LD M256                  ; 常圧制御要求(加圧中)
AND M310                 ; 常圧制御完了
OUT M260                 ; 加圧制御完了
;
MCR
END
ENDH
