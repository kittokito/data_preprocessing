DEVICE:53
;MODULE:R_Always
;MODULE_TYPE:0
;現在機種部品ﾜｰｸNo算出/Calculation of current model part product number
LD CR2002                ; 常時ON
LDA EM30010              ; TP)機種選択番号(1-10)
CON
SUB #1
CON
MUL #25
CON
ADD ZF5                  ; R現在部品番号(1〜25）
CON
SUB #1
CON
STA Z1
CON
MOV DM200:Z1 ZF3         ; 機種1部品1ﾜｰｸNo  R現在機種部品ﾜｰｸNo
END
ENDH
