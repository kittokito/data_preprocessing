DEVICE:53
;MODULE:CON_WD_PUSH
;MODULE_TYPE:1
;OneSystem SupportMacro
;
;-----------------------------------------
;Macro Name 	: 	IAI_CON_WD_PUSH
;Ver.		:	2.0
;Macro Type	:	Self Holding
;Function	:	IAI CON系直値PUSH動作ﾏｸﾛ
;-----------------------------------------
;引数：
;V0	=	目標位置(0.01mm)				V0	=	Target Position Value
;V1	=	押付け幅(0.01mm)				V1	=	Push Wide
;V2	=	速度							V2	=	Speed
;V3	=	加減速(0.01G)					V3	=	G
;V4　=	押付け電流値(#0〜255)			V4	=	Push Power
;
;P0	=	RB入力先頭番地	DM番号で指定	P0	=	Input Leading Address for RB
;P1	=	RB出力先頭番地	DM番号で指定	P1	=	Output Leading Address for RB
;P2	=	完了コード						P2	=	Macro Complete Code
;			#20			=	正常終了				#20			=	No Error
;			#21			=	RB応答なし				#21			=	Non Responce
;			#22			=	ｽﾃｰﾀｽ異常				#22			=	Status Abnormal
;			#23			=	押付け空振り			#23			=	Push Fail
;			#Alarmcode	=	Alarm					#Alarmcode	=	Alarm
;P3	=	離脱信号						P3	=	break off
;P4	=	押付け完了位置情報(0.01mm				P4	=	Push Result Value
;
;機能：CON系 MSEP 数直移動溶着用押付け指令ﾏｸﾛ/ Robot Move Macro for Numerical Push Move Mode For Welding
;IN
BLD P0:#7 #0             ; RB_IN
OUT PEND                 ; IN PEND
BLD P0:#7 #1             ; RB_IN
OUT HEND                 ; IN HEND
BLD P0:#7 #2             ; RB_IN
OUT MOVE                 ; IN MOVE
BLD P0:#7 #3             ; RB_IN
OUT ALM                  ; IN ALM
BLD P0:#7 #4             ; RB_IN
OUT SV                   ; IN SV
BLD P0:#7 #5             ; RB_IN
OUT PSFL                 ; IN PSFL   押付空振り
BLD P0:#7 #7             ; RB_IN
OUT ALML                 ; IN ALML
BLD P0:#7 #8             ; RB_IN
OUT M_END                ; IN MEND
BLD P0:#7 #12            ; RB_IN
OUT ZONE1                ; IN ZONE1
BLD P0:#7 #13            ; RB_IN
OUT ZONE2                ; IN ZONE2
BLD P0:#7 #14            ; RB_IN
OUT CRDY                 ; IN CRDY
BLD P0:#7 #15            ; RB_IN
OUT EMGS                 ; IN EMGS
;OUT
LD CSTR                  ; OUT START
BOUT P1:#7 #0            ; RB_OUT
LD HOME                  ; OUT HOME
BOUT P1:#7 #1            ; RB_OUT
LD STOP                  ; OUT STOP
BOUT P1:#7 #2            ; RB_OUT
LD RSET                  ; OUT RESET
BOUT P1:#7 #3            ; RB_OUT
LD JISL                  ; OUT ON=JOG
BOUT P1:#7 #5            ; RB_OUT
LD JOG_Minus             ; JOG-
BOUT P1:#7 #7            ; RB_OUT
LD JOG_Plus              ; JOG+
BOUT P1:#7 #8            ; RB_OUT
LD PUSH                  ; OUT Push
BOUT P1:#7 #12           ; RB_OUT
LD DIR                   ; OUT Push ON=+Dir
BOUT P1:#7 #13           ; RB_OUT
LD Incre                 ; OUT ON=INC
BOUT P1:#7 #14           ; RB_OUT
LD BKRL                  ; OUT Brake Release
BOUT P1:#7 #15           ; RB_OUT
;
LD MR19915               ; _運転ﾓｰﾄﾞ指定
ORP MR6104               ; _非常停止中　ﾓｰﾄﾞﾌﾗｸﾞ
@SET @R012               ; 終了処理
;初期化　/ Initialize
LD @CR2008
SET @R000
;RBｽﾃｰﾀｽﾓﾆﾀｰ / Status Monitoring
LDB EMGS                 ; IN EMGS
AND SV                   ; IN SV
AND CRDY                 ; IN CRDY
AND HEND                 ; IN HEND
ANB MOVE                 ; IN MOVE
OUT @R014                ; RB Status OK
;Move Parameter storage
STG @R000
MPS
LDA.D V1                 ; Push Wide
CON
STA.D P1:#2              ; RB_OUT
CON
LDA.D V0                 ; POS
CON
STA.D P1:#0              ; RB_OUT
CON
LDA V2                   ; SPEED
CON
STA P1:#4                ; RB_OUT
CON
LDA V3                   ; G
CON
STA P1:#5                ; RB_OUT
MRD
SET DIR                  ; OUT Push ON=+Dir
CON
SET PUSH                 ; OUT Push
CON
LDA V4                   ; Push_Power
CON
STA P1:#6                ; RB_OUT
MPP
TMH @0 #3
CON
AND @T0
JMP @R001
STG @R001
MPS
LD @R014                 ; RB Status OK
ANB ALM                  ; IN ALM
ANL
SET CSTR                 ; OUT START
CON
JMP @R002
MRD
ANB @R014                ; RB Status OK
DW #22 @DM0
CON
JMP @R012                ; 終了処理
MPP
AND ALM                  ; IN ALM
DW #21 @DM0
CON
JMP @R011                ; Alarm
STG @R002
MPS
LDB ALM                  ; IN ALM
ANB @T2
ANB PEND                 ; IN PEND
ANL
RES CSTR                 ; OUT START
CON
JMP @R003
MRD
AND ALM                  ; IN ALM
RES CSTR                 ; OUT START
CON
DW #21 @DM0
CON
JMP @R011                ; Alarm
MPP
TMR @2 #30
CON
AND @T2
RES CSTR                 ; OUT START
CON
DW #21 @DM0
CON
JMP @R011                ; Alarm
STG @R003
MPS
ANB ALM                  ; IN ALM
MPS
LDB PSFL                 ; IN PSFL   押付空振り
AND PEND                 ; IN PEND
AND P3                   ; break_off
ANL
DW #20 @DM0
CON
JMP @R004
MRD
LD PSFL                  ; IN PSFL   押付空振り
AND P3                   ; break_off
ANL
DW #20 @DM0
CON
JMP @R004
MPP
LD PSFL                  ; IN PSFL   押付空振り
AND CR2003               ; 常時OFF
ANL
DW #23 @DM0
CON
JMP @MR315               ; ﾏｸﾛ       終了処理
MPP
AND ALM                  ; IN ALM
JMP @R011                ; Alarm
STG @R004
@MOV.D P0 P4             ; RB_IN  Push_result
TMH @1 #0
CON
AND @T1
JMP @R012                ; 終了処理
;Alarm code Stroge
STG @R011                ; Alarm
LDA P1:#6                ; RB_OUT
CON
STA P2                   ; C_CODE
CON
JMP @R012                ; 終了処理
;終了処理
STG @R012                ; 終了処理
RES DIR                  ; OUT Push ON=+Dir
CON
RES PUSH                 ; OUT Push
CON
LDA @DM0
CON
STA P2                   ; C_CODE
CON
MPS
ZRES @R001 @R100         ; ﾏｸﾛ異常発生
CON
ENDS
MPP
MEND
END
ENDH
