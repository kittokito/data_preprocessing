SET MR9500               ; RB1 EMG 解除
CON
SET MR9501               ; RB2 EMG 解除
CON
SET MR9502               ; RB3 EMG 解除
CON
SET MR9503               ; RB4 EMG 解除
CON
SET MR9504               ; RB5 EMG 解除
CON
SET MR9505               ; RB6 EMG 解除
CON
SET MR9506               ; RB7 EMG 解除
CON
SET MR9602               ; RB10 EMG 解除
CON
SET MR9603               ; RB11 EMG 解除
CON
SET MR9605               ; RB13 EMG 解除
CON
SET MR9607               ; RB15 EMG 解除
CON
SET MR9609               ; RB19 EMG 解除
CON
SET MR004                ; ﾃﾞﾊﾞｯｸ用？？
CON
ZRES DM60000 DM60049     ; TaskNo /ERROR_DISP
;
;
LD CR2002                ; 常時ON
SET DM11211.15           ; RB13 排出品 記憶FLG
SET DM11212.15           ; RB14 不良品 記憶FLG
SET DM11213.15           ; RB15 不良品 記憶FLG
SET DM11214.15           ; RB16 不良品FLG
SET DM11215.15           ; RB17 不良品 記憶FLG
