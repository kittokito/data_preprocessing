AND<> V1 #100            ; Task_No
MPS
LDA V0                   ; Group_No
CON
SUB #1
CON
MUL #32
CON
ADD V1                   ; Task_No
CON
STA Z9
MRD
LDA V0                   ; Group_No
CON
SUB #1
CON
STA Z8
MPP
LD R44200:Z8             ; _Grp1 ﾎﾟｰｽﾞ要求
ANB CR2002               ; 常時ON
ANL
SET R42600:Z9            ; _Grp1_Task0_ﾎﾟｰｽﾞ許可
;<h1/>Exclusive/排他占有
;・排他解除待ち中にPAUSE要求にてHALT実施
LDB R59915               ; _運転ﾓｰﾄﾞ指定
LDA V0                   ; Group_No
CON
SUB #1
CON
STA Z8
CON
ANB R46104               ; _非常停止中　ﾓｰﾄﾞﾌﾗｸﾞ
ANB R48000:Z8            ; _Group1　ｴﾗｰ処理排他
ANB R46208:Z8            ; _Group1　ｴﾗｰ発生中
SET R48000:Z8            ; _Group1　ｴﾗｰ処理排他
MPS
SET @R000                ; ﾏｸﾛ実行中
CON
SET @R001                ; ErrorCode作成
MPP
AND CR2002               ; 常時ON
RES R54700:Z8            ; _ﾀｲﾑｱｯﾌﾟｴﾗｰ選択肢表示／非表示
;<h1/>Error Code Making/ｴﾗｰｺｰﾄﾞ作成
;ﾀｲﾑｱｯﾌﾟｴﾗｰ番号はｸﾞﾙｰﾌﾟ番号+5000固定
STG @R001                ; ErrorCode作成
MPS
LDA V0                   ; Group_No
CON
SUB #1
CON
MUL #2
CON
STA Z8
MRD
LDA V0                   ; Group_No
CON
MUL #10000
CON
STA @EM3                 ; ｸﾞﾙｰﾌﾟ番号桁揃え
MRD
LDA @EM3                 ; ｸﾞﾙｰﾌﾟ番号桁揃え
CON
ADD #5000
CON
STA @EM4                 ; 表示ｴﾗｰ番号
MRD
MOV @EM4 DM8080:Z8       ; 表示ｴﾗｰ番号  _ﾀｲﾑｱｯﾌﾟｴﾗｰ　ｴﾗｰ番号
MRD
FMOV #0 DM5800 #256      ; _ﾀｲﾑｱｯﾌﾟｴﾗｰ(ID1)ｱﾗｰﾑ検出ﾜｰﾄﾞﾋﾞｯﾄ
MPP
JMP @R002                ; ｾﾝｻｱﾗｰﾑﾋﾞｯﾄ作成
;<h1/>Error Message Alarm Bit Making/ｴﾗｰﾒｯｾｰｼﾞｱﾗｰﾑﾋﾞｯﾄ作成
STG @R002                ; ｾﾝｻｱﾗｰﾑﾋﾞｯﾄ作成
AND @R008                ; word-bit 作成
RES @R008                ; word-bit 作成
CON
JMP @R003                ; WaitError表示
;'センサ番号(アラームID1)ONビット生成
;@EM7 = V2 / 16
;@EM8 = V2 MOD 16
;
;BSET (KtoV_TERR_ALM:@EM7,@EM8)
;
;SET (@R008)
LD @R002                 ; ｾﾝｻｱﾗｰﾑﾋﾞｯﾄ作成
NCJ #1000
;'センサ番号(アラームID1)ONビット生成
;
;@EM7 = V2 / 16
;
LD CR2002                ; 常時ON
LDA V2                   ; SensorCode
CON
EXT
CON
EXT.L
CON
DIV.L +16
CON
STA @EM7
;@EM8 = V2 MOD 16
;
LD CR2002                ; 常時ON
LDA V2                   ; SensorCode
CON
EXT
CON
EXT.L
CON
DIV.L +16
CON
LDA.L TM2
CON
STA @EM8
;BSET (KtoV_TERR_ALM:@EM7,@EM8)
;
LD CR2002                ; 常時ON
LDA @EM7
CON
EXT
CON
STA.L Z12
BSET DM5800:Z12 @EM8     ; _ﾀｲﾑｱｯﾌﾟｴﾗｰ(ID1)ｱﾗｰﾑ検出ﾜｰﾄﾞﾋﾞｯﾄ
;SET (@R008)
LD CR2002                ; 常時ON
SET @R008                ; word-bit 作成
LABEL #1000
;
