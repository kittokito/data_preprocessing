DEVICE:53
;MODULE:ERROR_DISP
;MODULE_TYPE:1
;SCRIPT_TYPE:
;OneSystem SupportMacro
;
;-----------------------------------------
;Macro Name 	: 	ERROR_DISP
;Macro Type	:	Self Holding
;Function	:	通常ｴﾗｰ要求
;-----------------------------------------
;
;Copylight(C) 2011 SEIKO EPSON Corp. All rights reserved.
;引数：
;	V0	=	表示ｸﾞﾙｰﾌﾟ番号			#1〜8
;	V1	=	ﾀｽｸ番号					#0〜31
;	V2	=	通常ｴﾗｰ番号				#0〜4095
;	V7	=	表示ｾﾝｻ番号				#0〜4095
;	V3	=	復帰SW1名称登録番号		#0〜100
;	V4	=	復帰SW2名称登録番号		#0〜100
;	V5	=	復帰SW3名称登録番号		#0〜100
;	V6	=	復帰SW4名称登録番号		#0〜100
;	P9	=	選択SW名称登録番号		#0〜100		(戻り値)
;	P8	=	ｴﾗｰ完了番号				#0〜4095	(戻り値)	
;機能：
;	指定ｸﾞﾙｰﾌﾟのTPに指定ｴﾗｰ番号、指定復帰SW名称にてｱﾗｰﾑ画面表示、選択された復帰SW登録番号を戻り値に格納する
LDB MR002
AND R59915
MOV #0 P9                ; ReturnSw_No
MOV #0 P8                ; ReturnErr_No
MEND
LD R46104
ZRES @R000 @R015
CON
MEND
;<h1/>Argument Range Check/引数範囲ﾁｪｯｸ
LD< V0 #1                ; Group_No
OR> V0 #8                ; Group_No
OR< V1 #0                ; Task_No
OR> V1 #100              ; Task_No
LD> V1 #31               ; Task_No
AND< V1 #100             ; Task_No
ORL
OR< V2 #0                ; Error_No
OR> V2 #4095             ; Error_No
OR< V7 #0                ; Sensor_No
OR> V7 #4095             ; Sensor_No
OR< V3 #0                ; SW1_No
OR> V3 #100              ; SW1_No
OR< V4 #0                ; SW2_No
OR> V4 #100              ; SW2_No
OR< V5 #0                ; SW3_No
OR> V5 #100              ; SW3_No
OR< V6 #0                ; SW4_No
OR> V6 #100              ; SW4_No
SET R40008
CON
SET @R015
;<h1/>Init/初期化
;戻り値初期化
;ｴﾗｰ計測ﾀｲﾏ起動
LD @CR2008
ORB @R000
LDB R59915
OR MR002
ANL
SET @R014
MOV #0 P9                ; ReturnSw_No
MOV #0 P8                ; ReturnErr_No
MOV #0 @EM6
FMOV #0 @EM7 #4
;<h1/>Pause Request/ﾎﾟｰｽﾞ要求
;・ﾏｸﾛ実行中にPAUSE要求にてHALT実施(ﾒｲﾝﾀｽｸCALL時のみ)
LDB R59915
OR MR002
AND<> V1 #100            ; Task_No
MPS
LDA V0                   ; Group_No
CON
SUB #1
CON
MUL #32
CON
ADD V1                   ; Task_No
CON
STA Z9
MRD
LDA V0                   ; Group_No
CON
SUB #1
CON
STA Z8
MPP
LD R44200:Z8
ANB CR2002
ANL
SET R42600:Z9
;<h1/>Exclusive/排他占有
;同一TPｴﾗｰ画面表示排他解放待ちで処理開始
LDB R59915
OR MR002
LDA V0                   ; Group_No
CON
SUB #1
CON
STA Z8
CON
ANB R46104
ANB R48000:Z8
ANB R46208:Z8
SET R48000:Z8
SET @R000
CON
SET @R001
;<h1/>Error Code Making/ｴﾗｰｺｰﾄﾞ作成
;・ｴﾗｰｺｰﾄﾞはｸﾞﾙｰﾌﾟ番号＋指定ｴﾗｰｺｰﾄﾞ(0000〜4096）の5桁構成とする
STG @R001
MPS
LDA V0                   ; Group_No
CON
MUL #10000
CON
STA @EM1
MRD
LDA @EM1
CON
ADD V2                   ; Error_No
CON
STA @EM2
MRD
FMOV #0 DM5500 #256
MRD
FMOV #0 DM5800 #256
MPP
JMP @R002
;<h1/>Return Choise Switch/復帰選択肢SW名称番号制御
;・指定された選択肢SW名称番号をSW1〜SW4の指定ﾃﾞﾊﾞｲｽに格納
;・名称番号が0ならばSWは非表示とする
STG @R002
MPS
LDA V0                   ; Group_No
CON
SUB #1
CON
MUL #10
CON
STA Z10
MRD
LDA V0                   ; Group_No
CON
SUB #1
CON
MUL #8
CON
STA Z9
MRD
AND= V3 #0               ; SW1_No
RES R53101:Z9
CON
MOV #0 DM8002:Z10
MRD
AND<> V3 #0              ; SW1_No
SET R53101:Z9
CON
MOV V3 DM8002:Z10        ; SW1_No
MRD
AND= V4 #0               ; SW2_No
RES R53102:Z9
CON
MOV #0 DM8003:Z10
MRD
AND<> V4 #0              ; SW2_No
SET R53102:Z9
CON
MOV V4 DM8003:Z10        ; SW2_No
MRD
AND= V5 #0               ; SW3_No
RES R53103:Z9
CON
MOV #0 DM8004:Z10
MRD
AND<> V5 #0              ; SW3_No
SET R53103:Z9
CON
MOV V5 DM8004:Z10        ; SW3_No
MRD
AND= V6 #0               ; SW4_No
RES R53104:Z9
CON
MOV #0 DM8005:Z10
MRD
AND<> V6 #0              ; SW4_No
SET R53104:Z9
CON
MOV V6 DM8005:Z10        ; SW4_No
MPP
JMP @R003
;<h1/>Error Message Alarm Bit Making/ｴﾗｰﾒｯｾｰｼﾞｱﾗｰﾑﾋﾞｯﾄ作成
;・ｴﾗｰｺｰﾄﾞ格納
;・ｴﾗｰｺｰﾄﾞ、ｾﾝｻｺｰﾄﾞに対応したｱﾗｰﾑﾋﾞｯﾄ作成(256word-bit出力)
STG @R003
MPS
LDA V0                   ; Group_No
CON
SUB #1
CON
MUL #10
CON
STA Z10
MRD
MOV @EM2 DM8000:Z10
MRD
MOV V2 DM8001:Z10        ; Error_No
MPP
AND @R010
RES @R010
CON
JMP @R004
;
;'エラー番号(アラームID0)ONビット生成
;
;@EM7 = V2 / 16
;@EM8 = V2 MOD 16
;
;BSET (KtoV_ERR_ALM:@EM7,@EM8)
;
;'センサ番号(アラームID1)ONビット生成
;IF V7<>0 THEN
;	@EM9 = (V7-4000) / 100
;	@EM10 = (V7-4000)-(@EM9*100)
;
;	BSET (KtoV_TERR_ALM:@EM9,@EM10)
;END IF
;SET (@R010)
LD @R003
NCJ #1000
;'エラー番号(アラームID0)ONビット生成
;
;@EM7 = V2 / 16
;
LD CR2002
LDA V2                   ; Error_No
CON
EXT
CON
EXT.L
CON
DIV.L +16
CON
STA @EM7
;@EM8 = V2 MOD 16
;
LD CR2002
LDA V2                   ; Error_No
CON
EXT
CON
EXT.L
CON
DIV.L +16
CON
LDA.L TM2
CON
STA @EM8
;BSET (KtoV_ERR_ALM:@EM7,@EM8)
;
LD CR2002
LDA @EM7
CON
EXT
CON
STA.L Z12
BSET DM5500:Z12 @EM8
;'センサ番号(アラームID1)ONビット生成
;
;IF V7<>0 THEN
;
LD<> V7 #0               ; Sensor_No
NCJ #2000
;	@EM9 = (V7-4000) / 100
;
LD CR2002
LDA V7                   ; Sensor_No
CON
EXT
CON
SUB.L +4000
CON
EXT.L
CON
DIV.L +100
CON
STA @EM9
;	@EM10 = (V7-4000)-(@EM9*100)
;
LD CR2002
LDA @EM9
CON
EXT
CON
MUL.L +100
CON
STA.L @VM0
CON
LDA V7                   ; Sensor_No
CON
EXT
CON
SUB.L +4000
CON
SUB.L @VM0
CON
STA @EM10
;	BSET (KtoV_TERR_ALM:@EM9,@EM10)
;
LD CR2002
LDA @EM9
CON
EXT
CON
STA.L Z12
BSET DM5800:Z12 @EM10
;END IF
;
LABEL #2000
;SET (@R010)
LD CR2002
SET @R010
LABEL #1000
;
;<h1/>Error Display/通常ｴﾗｰ画面表示
STG @R004
MPS
LDA V0                   ; Group_No
CON
SUB #1
CON
STA Z8
CON
SET R46208:Z8
MPP
TMH @1 #5
CON
AND @T1
LDA V0                   ; Group_No
CON
SUB #1
CON
MUL #8
CON
STA Z9
CON
SET R53100:Z9
CON
JMP @R005
;<h1/>Error Display Release Waiting/通常ｴﾗｰ画面解除待ち
;・ｴﾗｰ復帰選択肢→STARTｽｲｯﾁ待ち
;・安全ｶﾊﾞｰ開、元圧低下、ｴﾘｱｾﾝｻ遮断全復帰待ち(ﾒｲﾝﾀｽｸの場合)
;・画面表示中のﾎﾟｰｽﾞ要求監視
STG @R005
MPS
LDA V0                   ; Group_No
CON
SUB #1
CON
MUL #10
CON
STA Z10
MRD
LDA V0                   ; Group_No
CON
SUB #1
CON
STA Z8
MPP
LD<> V1 #100             ; Task_No
ANB R46200:Z8
ANB R50700:Z8
ANB R46300:Z8
OR= V1 #100              ; Task_No
AND<> DM8006:Z10 #0
LD= V0 #1                ; Group_No
ANP R44400
LD= V0 #2                ; Group_No
ANP R44600
ORL
LD= V0 #3                ; Group_No
ANP R44800
ORL
LD= V0 #4                ; Group_No
ANP R45000
ORL
LD= V0 #5                ; Group_No
ANP R45200
ORL
LD= V0 #6                ; Group_No
ANP R45400
ORL
LD= V0 #7                ; Group_No
ANP R45600
ORL
LD= V0 #8                ; Group_No
ANP R45800
ORL
ANL
ANL
JMP @R006
;<h1/>Return Choise Switch Check/復帰選択肢SW確認
;・選択された復帰SW番号を戻り値に格納
;・ｴﾗｰ復帰選択肢は選択番号では無く、[名称登録番号]とする
LD @R006
MPS
LDA V0                   ; Group_No
CON
SUB #1
CON
MUL #10
CON
STA Z10
MRD
AND= DM8006:Z10 #1
MOV V3 @EM6              ; SW1_No
MRD
AND= DM8006:Z10 #2
MOV V4 @EM6              ; SW2_No
MRD
AND= DM8006:Z10 #3
MOV V5 @EM6              ; SW3_No
MPP
AND= DM8006:Z10 #4
MOV V6 @EM6              ; SW4_No
STG @R006
AND<> @EM6 #0
MPS
LDA V0                   ; Group_No
CON
SUB #1
CON
MUL #8
CON
STA Z9
MRD
RES R53100:Z9
MPP
JMP @R007
;<h1/>Data Collection Making/ﾃﾞｰﾀ収集用ﾃﾞｰﾀ作成
;・ｴﾗｰｺｰﾄﾞ
;・ｴﾗｰ停止時間
;・復帰選択肢
;///Ver1.0.2///
;・データ収集エラー復帰選択肢格納(DC_Err*Ret)に選択肢が格納されない不具合修正
STG @R007
MPS
LDA V0                   ; Group_No
CON
SUB #1
CON
MUL #10
CON
STA Z7
MRD
MOV @EM2 DM5080:Z7
MRD
MOV.D @EM4 DM5081:Z7
MRD
MOV @EM6 DM5083:Z7
MRD
MOV #0 DM5084:Z7
MRD
MOV #0 DM5085:Z7
MRD
MOV #0 DM5086:Z7
MRD
SET R46308:Z8
MPP
RES @R014
CON
JMP @R008
;<h1/>End Process/終了処理
;通常ｴﾗｰ排他開放、初期化
STG @R008
MPS
LDA V0                   ; Group_No
CON
SUB #1
CON
MUL #20
CON
STA Z7
MRD
LDA V0                   ; Group_No
CON
SUB #1
CON
STA Z8
MRD
LDA V0                   ; Group_No
CON
SUB #1
CON
MUL #8
CON
STA Z9
MRD
LDA V0                   ; Group_No
CON
SUB #1
CON
MUL #10
CON
STA Z10
MPP
AND<> DM3000:Z7 #1010
FMOV #0 DM5500 #256
FMOV #0 DM5500 #256
FMOV #0 DM8000:Z10 #7
MPS
ZRES R53101:Z9 R53104:Z9
CON
RES R46208:Z8
CON
RES R48000:Z8
MRD
MOV @EM6 P9              ; ReturnSw_No
MRD
MOV V2 P8                ; Error_No  ReturnErr_No
MRD
MEND
MPP
RES @R000
CON
ENDS
;<h1/>Error M/C Stop Time/ｴﾗｰ停止時間計測
LD @R014
MPS
ANB @T0
TMS @0 #4294967295
MPP
LDA.D #4294967295
CON
SUB.D @T0
CON
STA.D @EM4
END
ENDH
