SET MR1301               ; 塗布RB1 緊急停止
CON
SET MR1501               ; 塗布RB2 緊急停止
CON
SET MR1701               ; 塗布RB3 緊急停止
CON
SET MR1901               ; 塗布RB4 緊急停止
CON
SET MR9500               ; RB1 EMG 解除
CON
SET MR9501               ; RB2 EMG 解除
CON
SET MR9502               ; RB3 EMG 解除
CON
SET MR9503               ; RB4 EMG 解除
CON
SET MR9504               ; RB5 EMG 解除
CON
SET MR9507               ; RB8 EMG 解除
CON
SET MR9600               ; RB7 EMG 解除
CON
SET MR9601               ; RB6 EMG 解除
CON
SET MR9604               ; RB11 EMG 解除
CON
SET MR9605               ; RB12 EMG 解除
CON
SET MR9606               ; RB13 EMG 解除
CON
RES MR004
CON
ZRES DM60000 DM60049     ; TaskNo /ERROR_DISP
END
ENDH
