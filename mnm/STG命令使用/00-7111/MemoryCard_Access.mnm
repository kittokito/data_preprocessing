LDP @MR100
NCJ #1000
;select case EM30010
;
LD CR2002                ; always on
MOV EM30010 @VM0         ; (TP)model select number(1-10)
;case 1 to 10	'Model No.1～10
;
LD>= @VM0 #1
OUT @VB00
LD<= @VM0 #10
AND @VB00
NCJ #2000
; 'ﾌｫﾙﾀﾞｰ名/Folder name
;
;  @EM100.T = "Data\"+EM30200.T:EM30080+"\"+EM30400.T:EM30090+"\Result"
;
LD CR2002                ; always on
LDA EM30090              ; Parts select data offset
CON
EXT
CON
STA.L Z12
CON
ADRSET EM30400:Z12 @VM1  ; component 1_component name
LD CR2002                ; always on
LDA EM30080              ; Model select data offset
CON
EXT
CON
STA.L Z12
LD CR2002                ; always on
ADRSET VM2 VM0
MOV #0 VM2
MOV.D VM0 @VM3
SMOV "Data\" *@VM3
LD CR2002                ; always on
SADD *@VM3 EM30200:Z12 *@VM3;model 1_model name
SADD *@VM3 "\" *@VM3
SADD *@VM3 *@VM1 *@VM3
SADD *@VM3 "\Result" *@VM3
SMOV *@VM3 @EM100
;	'書込みﾃﾞｰﾀ数を設定/Set the number of write data
;
;    IF MR9915 = ON THEN
;
LD MR9915                ; Old choke release selection
NCJ #2001
;	@DM40.U = #214
;
LD CR2002                ; always on
MOV #214 @DM40
;	END IF
;
LABEL #2001
;	IF MR9915 = OFF THEN
;
LDB MR9915               ; Old choke release selection
NCJ #2002
;	@DM40.U = #217
;
LD CR2002                ; always on
MOV #217 @DM40
;	END IF
;
LABEL #2002
LD CR2002                ; always on
CJ #2003
LABEL #2000
;case else
;
;	'ﾌｫﾙﾀﾞｰ名/Folder name
;
;	@EM100.T = "Data\NoModel\Result"
;
LD CR2002                ; always on
SMOV "Data\NoModel\Result" @EM100
;	'書込みﾃﾞｰﾀ数を設定/Set the number of write data
;
;    IF MR9915 = ON THEN
;
LD MR9915                ; Old choke release selection
NCJ #2004
;	@DM40.U = #214
;
LD CR2002                ; always on
MOV #214 @DM40
;	END IF
;
LABEL #2004
;	IF MR9915 = OFF THEN
;
LDB MR9915               ; Old choke release selection
NCJ #2005
;	@DM40.U = #217
;
LD CR2002                ; always on
MOV #217 @DM40
;	END IF
;
LABEL #2005
;end select
;
LABEL #2003
;'ﾌｧｲﾙ名作成(年月日部分) 文字列生成
;
;'File name creation (year, month, day part) String generation
;
;@DM1 = ASC(TBCD(CM700))	'年/Year
;
LD CR2002                ; always on
LDA CM700                ; calendar timer (Y)
CON
TBCD
CON
ASC
CON
STA @DM1
;@DM2 = ASC(TBCD(CM701))	'月/month
;
LD CR2002                ; always on
LDA CM701                ; calendar timer (M)
CON
TBCD
CON
ASC
CON
STA @DM2
;@DM3 = ASC(TBCD(CM702))	'日/day
;
LD CR2002                ; always on
LDA CM702                ; calendar timer (D)
CON
TBCD
CON
ASC
CON
STA @DM3
;@DM4 = $0000				'文字列 Nullｺｰﾄﾞ/String Null code
;
LD CR2002                ; always on
MOV $0000 @DM4
;'ﾌｧｲﾙ名を作成/Create file name
;
; @DM1.T = "Log_Res_" + @DM1.T
;
LD CR2002                ; always on
ADRSET VM2 VM0
MOV #0 VM2
MOV.D VM0 @VM5
SMOV "Log_Res_" *@VM5
LD CR2002                ; always on
SADD *@VM5 @DM1 *@VM5
SMOV *@VM5 @DM1
;'保存形式を設定   6:16bit符号付きﾊﾞｲﾅﾘｰﾃﾞｰﾀ
;
;'Set storage format 6: 16 bit signed binary data
;
;@DM30 = 6
;
LD CR2002                ; always on
MOV #6 @DM30
;'書込みﾓｰﾄﾞを設定 0:追加書込
;
;'Set write mode 0: Additional write
;
;@DM31 = 0
;
LD CR2002                ; always on
MOV #0 @DM31
;'行末処理を設定   1:CRLF
;
;'Set end of line processing 1: CRLF
;
;@DM32 = 1
;
LD CR2002                ; always on
MOV #1 @DM32
