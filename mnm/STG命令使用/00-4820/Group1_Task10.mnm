;
;<h1/>MC AXIS No.割出
;IF RB_TYPE[2,DM50312]<> 0 THEN
;		@DM14.T =STR(RB_TYPE[2,DM50312]) 'MC AxisNo.割出
;		DISB(@DM14,@DM16,2) 
;		BSWAP(@D16,2)             '桁分割
;		Axis_No= RDASC(@DM16.T)		     'AXIS No.格納
